    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.37.1",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^1.3.2",
    "express-rate-limit": "^3.5.0",
